generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  username String @db.VarChar(120)
  password String @db.VarChar(120)
  role     Roles
  id       Int    @id @default(autoincrement())
}

enum Roles {
  Readonly
  User
  Admin
}
